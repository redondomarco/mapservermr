                CONNECTION "/etc/mapserver/seguridad/VILLA_POLLEDO-ZR.kml"
                DATA "VILLA POLLEDO-ZR.kmz"
                CLASS
                        NAME "VILLA POLLEDO-ZR.kmz"
                        STYLE
                                #COLOR 255 100 100
                                OUTLINECOLOR 255 100 100
                                SIZE 5
                                symbol 'poligono_grueso'
                        END     
                END
END

