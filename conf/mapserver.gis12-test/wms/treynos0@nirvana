MAP # 
	STATUS ON
	NAME "SEGURIDAZADAS"
	EXTENT 5422163.0 6344000.0 5446837.0 6364000.0
	SIZE 768 647
	UNITS METERS
	FONTSET "/etc/mapserver/fonts.list.example"
	IMAGETYPE jpeg
	IMAGECOLOR 242 242 242
	RESOLUTION 75
	SHAPEPATH "/geodata/"
	SYMBOLSET "/etc/mapserver/infomapas.sym"
	Symbol NAME "poligono_grueso" TYPE ELLIPSE FILLED TRUE POINTS 1 1 END END
	OUTPUTFORMAT
		NAME "jpeg"
		DRIVER "GD/JPEG"
		FORMATOPTION "QUALITY=90"
		MIMETYPE "image/jpeg"
		IMAGEMODE RGBA
		EXTENSION "jpg"
	END


WEB
	IMAGEPATH "/var/www/mapserver"
	IMAGEURL "mapserver/"
	METADATA
            "wms_title"  "WMS-SEGURIDAZADAS"  ##required
            "wms_name"  "WMS-SEGURIDAZADAS"           
            "wms_abstract" "WMS-SEGURIDAZADAS"        
            "wms_onlineresource" "http://infomapa.rosario.gov.ar/wms/internas?"
            "wms_srs" "epsg:22185 epsg:4326"
            "wms_extent" "5422163.0 6344000.0 5446837.0 6364000.0"
            'wms_feature_info_mime_type' "text/html"
        END

        EMPTY "http://infomapa.rosario.gov.ar/emapa/datosUtiles.htm"
        ERROR "http://infomapa.rosario.gov.ar/emapa/error.jsp"

END
PROJECTION
  "init=epsg:22185"   ##required
END

	LAYER # Dengue Areas Trabajadas
		NAME "9998"
		PROCESSING "CLOSE_CONNECTION=DEFER"
		METADATA
		    "wms_title"    "Dengue Areas Trabajadas"  ##required
		    "wms_srs" "EPSG:4326 epsg:22185"
		    "wms_extent" "5422163.0 6344000.0 5446837.0 6364000.0"
		END
		PROJECTION
		  "init=epsg:22185"
		END
		CONNECTION "@gis"
		CONNECTIONTYPE INFORMIXSPATIAL
		PROCESSING "CLOSE_CONNECTION=DEFER" 
		DATA "the_geom from dengue_areas USING UNIQUE SE_ROW_ID FILTER"
		STATUS ON
		TYPE POLYGON
		POSTLABELCACHE OFF
		LABELCACHE ON
		TOLERANCEUNITS PIXELS
		TOLERANCE 5
		GROUP "Salud"
		TRANSPARENCY 60

		CLASS
			NAME "Descacharrización, Búsqueda de Febriles y Fumigación "
			EXPRESSION ('[ACCION]' = 'DBF')
			STYLE 
				SIZE 2
				OUTLINECOLOR 0 0 0
				COLOR 0 255 0
			END
		END

		
		CLASS
			NAME "Descacharrización y Búsqueda de Febriles"
			EXPRESSION ('[ACCION]' = 'DB')
			STYLE 
				SIZE 2
				OUTLINECOLOR 0 0 0
				COLOR 255 255 0
			END
		END


		CLASS
			NAME "Fumigación "
			EXPRESSION ('[ACCION]' = 'D')
			STYLE 
				SIZE 2
				OUTLINECOLOR 0 0 0
				COLOR 255 0 0
			END
		END

	END

