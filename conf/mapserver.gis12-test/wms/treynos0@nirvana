	LAYER # Comercios Ord 8869/2011
		NAME "58"
                PROCESSING "CLOSE_CONNECTION=DEFER"
                METADATA
                    "wms_title"    "Comercios Ord 8869/2011"  ##required
                    "wms_srs" "EPSG:4326 epsg:22185"
                    "wms_extent" "5422163.0 6344000.0 5446837.0 6364000.0"
		    "wms_include_items" "all"
                END
                PROJECTION
                  "init=epsg:22185"
                END
		CONNECTION "@gis"
		CONNECTIONTYPE INFORMIXSPATIAL
		DATA "the_geom from v_comercios USING UNIQUE NRO_COM"
		STATUS ON
		TYPE POINT
		POSTLABELCACHE OFF
		LABELCACHE OFF
		LABELITEM "nro_com"
		MAXSCALE 15000.0
		GROUP "COMERCIOS"
                TEMPLATE "content_comercios_template.html"
                HEADER "header_lugaresinteres_template.html"
                FOOTER "footer_lugaresinteres_template.html"
                TOLERANCEUNITS PIXELS
                TOLERANCE 10


		CLASS
			NAME "Comercios Ord 8869/2011"

			LABEL
				COLOR 0 0 0
				TYPE TRUETYPE
				ANGLE 0.0
				ANTIALIAS OFF
				FONT arialbd
				SIZE 8
				BUFFER 0
				MAXSIZE 9
				MINDISTANCE 0
				PARTIALS OFF
				POSITION CC
				FORCE TRUE
			END

			STYLE 
				SYMBOL "circle"
				SIZE 3
				OUTLINECOLOR 150 0 0
				COLOR 150 0 0
			END
		END

	END

